<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>Dashboard</h1>

    <ul>
      <% users.forEach((user) => { %>
      <li>
        <span><%= user.username %></span>
        <a href="/users/<%= user.id %>">View</a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
